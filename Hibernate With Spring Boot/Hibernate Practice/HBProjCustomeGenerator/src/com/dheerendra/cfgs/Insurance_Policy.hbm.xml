<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate?Hibernate Mapping DTD//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.dheerendra.domain.Insurance_Policy" table="INSURANCE_POLICY" lazy="true" proxy="com.dheerendra.domain.Insurance_Policy">
    <!-- singular Id Field-->
    <id name="policyId" column="POLICYID">
    	<generator class="com.dheerendra.generator.CustomGenerator"></generator>
    	</id>
    <property name="policyName" column="POLICYNAME"/>
    <property name="tenure" column="TENURE"/>
    <property name="premium" column="PREMIUM"/>
  </class> 

</hibernate-mapping>  