package com.arzds.model;

public class Employee {
private String nm;
private int id;
private double sal;
public String getNm() {
	return nm;
}
public void setNm(String nm) {
	this.nm = nm;
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public double getSal() {
	return sal;
}
public void setSal(double sal) {
	this.sal = sal;
}
@Override
public String toString() {
	return "Employee [nm=" + nm + ", id=" + id + ", sal=" + sal + "]";
}

}
